{
  "kind": "collectionType",
  "collectionName": "ninepmx_blogs",
  "info": {
    "singularName": "ninepmx-blog",
    "pluralName": "ninepmx-blogs",
    "displayName": "ninepmx-blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "likes": {
      "type": "integer"
    },
    "views": {
      "type": "integer"
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "slug": {
      "type": "customField",
      "options": {
        "pattern": "title"
      },
      "customField": "plugin::slug.slug"
    },
    "ninepmx_comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ninepmx-comment.ninepmx-comment",
      "mappedBy": "ninepmx_blog"
    }
  }
}
