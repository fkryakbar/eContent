{
  "kind": "collectionType",
  "collectionName": "ninepmx_projects",
  "info": {
    "singularName": "ninepmx-project",
    "pluralName": "ninepmx-projects",
    "displayName": "ninepmx-project"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "customField",
      "options": {
        "pattern": "title"
      },
      "customField": "plugin::slug.slug"
    },
    "likes": {
      "type": "integer"
    },
    "views": {
      "type": "integer",
      "required": false
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "project_link": {
      "type": "string"
    },
    "github_link": {
      "type": "string"
    },
    "ninepmx_comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ninepmx-comment.ninepmx-comment",
      "mappedBy": "ninepmx_project"
    }
  }
}
